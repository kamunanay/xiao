{
  "name": "xis-whatsapp-bot",
  "version": "3.5.0",
  "description": "WhatsApp Multi-Device Bot dengan Fitur Lengkap: AI, TikTok, Spotify, Brat, Lacak Nomor, Group Management, dll",
  "main": "xiao.js",
  "scripts": {
    "start": "node xiao.js",
    "dev": "nodemon xiao.js",
    "reset": "rm -rf sessions group_settings.json && node xiao.js",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "update": "npm update",
    "test": "echo \"No tests specified\" && exit 0",
    "fix": "rm -rf sessions group_settings.json && npm start",
    "install-all": "npm install --save @whiskeysockets/baileys axios pino qrcode-terminal",
    "repair": "rm -rf sessions node_modules package-lock.json && npm install",
    "setup": "npm install && node xiao.js",
    "backup": "cp -r sessions sessions_backup && cp group_settings.json group_settings_backup.json"
  },
  "keywords": [
    "whatsapp-bot",
    "baileys",
    "whatsapp-api",
    "spotify-bot",
    "tiktok-downloader",
    "ai-chatbot",
    "termux-bot",
    "group-management",
    "whatsapp-automation"
  ],
  "author": "小舞 Ga",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@whiskeysockets/baileys": "^6.5.0",
    "axios": "^1.6.0",
    "pino": "^8.15.0",
    "qrcode-terminal": "^0.12.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}