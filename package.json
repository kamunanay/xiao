{
  "name": "xis-whatsapp-bot",
  "version": "3.0.0",
  "description": "WhatsApp Multi-Device Bot dengan Pairing Code 8 Digit dan Fitur Lengkap",
  "main": "xiao.js",
  "scripts": {
    "start": "node xiao.js",
    "dev": "nodemon xiao.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "fix-sessions": "rm -rf sessions && mkdir sessions",
    "reset": "rm -rf sessions && npm start",
    "clear": "clear && node xiao.js",
    "update": "npm update && npm audit fix",
    "install-all": "npm install --save @whiskeysockets/baileys axios pino qrcode-terminal"
  },
  "keywords": [
    "whatsapp-bot",
    "baileys",
    "multi-device",
    "whatsapp-api",
    "bot",
    "automation",
    "pairing-code"
  ],
  "author": "小舞 Ga",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kamunanay/xiao.git"
  },
  "bugs": {
    "url": "https://github.com/kamunanay/xiao/issues"
  },
  "homepage": "https://github.com/kamunanay/xiao#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@whiskeysockets/baileys": "^6.5.0",
    "axios": "^1.6.0",
    "pino": "^8.15.0",
    "qrcode-terminal": "^0.12.0",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.0",
    "moment-timezone": "^0.5.43",
    "node-cache": "^5.1.2",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^6.0.3"
  },
  "peerDependencies": {
    "node-gyp": "^9.0.0"
  },
  "config": {
    "port": 3000,
    "session_timeout": 86400000,
    "max_file_size": 100000000
  },
  "files": [
    "xiao.js",
    "setting.js",
    "sessions/",
    "README.md",
    "LICENSE"
  ],
  "directories": {
    "lib": "lib"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}